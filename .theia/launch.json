{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Servidor HTTP",
      "type": "python",
      "request": "launch",
      "module": "flask",
      "env": {
        "FLASK_APP": "${workspaceFolder}/http/servidor"
      },
      "args": ["run", "--no-debugger", "--no-reload"]
    },
    {
      "name": "Python: cliente TSDB",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/tsdb/cliente-influxdata.py",
      "console": "integratedTerminal"
    }
  ]
}
